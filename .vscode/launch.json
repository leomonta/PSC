{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "PSCClient",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/client/bin/PSCC",
			"args": [ ],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/client/bin/",
			"environment": [ ],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"setupCommands": [
				{
					"description": "Enable reformatting for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": false
				}
			],
			"preLaunchTask": "PSCServer build all modified files"
		},
		{
			"name": "PSCServer",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/server/bin/PSCS",
			"args": [ ],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/server/bin/",
			"environment": [ ],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"setupCommands": [
				{
					"description": "Enable reformatting for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": false
				}
			],
			"preLaunchTask": "PSCServer build all modified files"
		},
		{
			"name": "Python: cpp builder",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"justMyCode": true,
			"cwd": "${workspaceFolder}/server"
		}
	]
}